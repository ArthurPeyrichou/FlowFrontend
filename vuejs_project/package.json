{
  "name": "vuejs_project",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "serve": "vue-cli-service serve --port 4200",
    "build": "vue-cli-service build",
    "test:unit": "vue-cli-service test:unit",
    "test:e2e": "vue-cli-service test:e2e --headless",
    "test:e2e-debug": "vue-cli-service test:e2e",
    "test:unit-coverage": "nyc npm run test:unit",
    "test:e2e-coverage": "vue-cli-service test:e2e --headless && npx nyc report --reporter=lcov",
    "lint": "vue-cli-service lint",
    "styleguide": "vue-cli-service styleguidist",
    "vue-docgen": "vue-docgen src/components/**/*.vue docs/components",
    "styleguide:build": "vue-cli-service styleguidist:build"
  },
  "dependencies": {
    "@babel/plugin-syntax-dynamic-import": "^7.8.3",
    "@babel/polyfill": "^7.10.1",
    "@fortawesome/fontawesome-free": "^5.13.0",
    "awesome-typescript-loader": "^5.2.1",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-preset-es2015": "^6.24.1",
    "bootstrap": "^4.5.0",
    "bootstrap-vue": "^2.15.0",
    "core-js": "^3.6.5",
    "d3": "^5.16.0",
    "js-loader": "^0.1.1",
    "vue": "^2.6.11",
    "vue-class-component": "^7.2.3",
    "vue-docgen-cli": "^4.25.0",
    "vue-mc": "^0.6.0",
    "vue-property-decorator": "^8.4.2",
    "vue-router": "^3.2.0",
    "webpack-cli": "^3.3.12",
    "webpack-node-externals": "^1.7.2"
  },
  "devDependencies": {
    "@cypress/browserify-preprocessor": "^3.0.0",
    "@cypress/code-coverage": "^3.8.1",
    "@types/chai": "^4.2.11",
    "@types/d3": "5.7.2",
    "@types/mocha": "^5.2.4",
    "@typescript-eslint/eslint-plugin": "^2.33.0",
    "@typescript-eslint/parser": "^2.33.0",
    "@vue/babel-preset-app": "^4.4.6",
    "@vue/cli-plugin-babel": "~4.4.0",
    "@vue/cli-plugin-e2e-cypress": "~4.4.0",
    "@vue/cli-plugin-eslint": "~4.4.0",
    "@vue/cli-plugin-router": "~4.4.0",
    "@vue/cli-plugin-typescript": "~4.4.0",
    "@vue/cli-plugin-unit-mocha": "~4.4.0",
    "@vue/cli-service": "~4.4.0",
    "@vue/eslint-config-typescript": "^5.0.2",
    "@vue/test-utils": "^1.0.3",
    "babel": "^6.23.0",
    "babel-plugin-istanbul": "^6.0.0",
    "babel-preset-vue": "^2.0.2",
    "chai": "^4.2.0",
    "clean-webpack-plugin": "^3.0.0",
    "cross-env": "^7.0.2",
    "eslint": "^6.7.2",
    "eslint-plugin-vue": "^6.2.2",
    "expect": "^26.1.0",
    "html-webpack-plugin": "^4.3.0",
    "imports-loader": "^1.1.0",
    "istanbul": "^0.4.5",
    "istanbul-instrumenter-loader": "^3.0.1",
    "jsdom": "^16.2.2",
    "jsdom-global": "^3.0.2",
    "mocha": "^8.0.1",
    "mochapack": "^2.0.2",
    "nyc": "^15.1.0",
    "typescript": "~3.9.3",
    "vue-cli-plugin-styleguidist": "~4.25.0",
    "vue-loader": "^15.9.3",
    "vue-styleguidist": "^4.25.0",
    "vue-template-compiler": "^2.6.11",
    "webpack": "^4.43.0"
